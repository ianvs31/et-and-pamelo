{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "admin.html", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" },
    { "src": "models/**", "use": "@vercel/static" },
    { "src": "api/**", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}


